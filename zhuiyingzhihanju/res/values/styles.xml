<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    
    
    
    <style name="MyText">
     
        <item name="android:layout_marginLeft">17dp</item>
      
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333</item>
    </style>
    
    
      
    <style name="MyName">
     
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333</item>
    </style>
    
     <style name="MySign">
      <item name="android:layout_marginTop">11dp</item>
      
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#bbb</item>
    </style>
    
    
     <style name="MySet">
   
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#333</item>
    </style>
    
      <style name="CircleText">
   
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#777</item>
    </style>
    
    
    
    <style name="anim_photo_select">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_to_bottom</item>
    </style>
    
    
    
    
      <style name="sNumeText">
   
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#7ac057</item>
    </style>
    
      <style name="stitleText">
   
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#333</item>
    </style>
    
      
      
       <style name="titleText">
   
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#191919</item>
    </style>
         <style name="desText">
   
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#999</item>
       
    </style>
    
      
      
      
      
      
      
      
      
      
    
</resources>
