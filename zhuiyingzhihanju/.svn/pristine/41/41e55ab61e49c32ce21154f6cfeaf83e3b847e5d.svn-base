<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/color_feedbackbg"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/ll_title_dialog"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="110dp"
        android:gravity="center"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/dialog_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/update_title"
            android:textColor="@color/green"
            android:textSize="18sp" />
    </LinearLayout>
	
    <TextView 
        android:id="@+id/dialog_message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="34dp"
        android:layout_marginRight="34dp"
        android:layout_marginTop="20dp"
        android:lineSpacingExtra="5dp"
        android:textSize="13sp"
        android:text=""
        android:textColor="#999999"/>
    

    
    <LinearLayout 
        android:id="@+id/dialog_ll_bottom"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:layout_marginTop="50dp"
        android:gravity="center">
        <TextView 
            android:id="@+id/dialog_cancle"
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:text="@string/update_cancel"
            android:background="@drawable/comm_dialog_button_selector"
            android:textColor="@drawable/comm_dialog_button_text_selector"
            android:textSize="14sp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="5dp"
            android:gravity="center"/>
        <TextView 
            android:id="@+id/dialog_enter"
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_weight="1"
            android:text="@string/update_sure"
            android:background="@drawable/up_btn"
            android:textColor="@drawable/comm_dialog_button_text_selector"
            android:textSize="14sp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="15dp"
            android:gravity="center"/>
    </LinearLayout>
    
</LinearLayout>