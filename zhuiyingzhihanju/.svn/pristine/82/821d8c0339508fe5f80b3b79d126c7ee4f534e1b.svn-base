<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >

    <include
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        layout="@layout/title" />
<ScrollView 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="none"
    
    >
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        >
     <RelativeLayout
        android:id="@+id/rl_wifi"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
     
        android:layout_marginTop="1dp"
        android:background="@drawable/setting_item_selector" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/setting_wifi"
              style="@style/MySet" />

        <ToggleButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:id="@+id/set_wifi"
          android:checked="true"  
        android:textOff=""  
        android:textOn="" 
            android:background="@drawable/setting_chekbox" />
    </RelativeLayout>
    
    
    
    <RelativeLayout
        android:id="@+id/rl_tuisong"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
       android:layout_marginTop="10dp"
     
        android:background="@drawable/setting_item_selector" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/setting_tui"
            style="@style/MySet"/>

        <ToggleButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
             android:id="@+id/set_tui"
                android:text=""
                 android:checked="true"  
        android:textOff=""  
        android:textOn="" 
            android:background="@drawable/setting_chekbox" />
    </RelativeLayout>
    
    
    
    
    
    
    
    <RelativeLayout
        android:id="@+id/rl_clear"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/setting_item_selector" >

        <TextView
            style="@style/MySet"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/setting_clear" />

        <ImageView
            android:id="@+id/clear_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/right"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/cache_size"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_marginRight="20dp"
            android:layout_toLeftOf="@id/clear_tag"
            android:gravity="center_vertical"
            android:text="0KB"
            android:textColor="#bbb"
            android:textSize="12sp" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
      
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#eee" />

    <RelativeLayout
        android:id="@+id/rl_version"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_item_selector" >

        <TextView
            style="@style/MySet"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="20sp"
            android:paddingRight="20dp"
            android:text="@string/setting_version" />

      
        
        <TextView
            android:id="@+id/rl_isNewest"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:gravity="center_vertical"
            android:text="@string/setting_new"
              android:layout_marginLeft="8dp"
            android:textColor="#bbb"
            android:textSize="12sp" />
          <ImageView 
            android:id="@+id/set_newdot"
              android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/news_dot"
            android:layout_toLeftOf="@id/rl_isNewest"
          
            android:layout_centerVertical="true"
            />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
       
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#eee" />

    <RelativeLayout
        android:id="@+id/rl_feedback"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:background="@drawable/setting_item_selector" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/right" />

        <TextView
            style="@style/MySet"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/setting_feedback" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
      
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#eee" />

    <RelativeLayout
        android:id="@+id/rl_Down"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/setting_item_selector" >

        <TextView
            style="@style/MySet"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/setting_down" />
        
       <TextView
            android:id="@+id/set_DownText"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:gravity="center_vertical"
           android:lines="2"
            android:textColor="#bbb"
           android:layout_centerVertical="true"
           
            android:textSize="12sp" />
        
        
        
        
        
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
     
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#eee" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
         android:visibility="gone"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:background="#eee" />

    <RelativeLayout
        android:id="@+id/rl_Change"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:background="@drawable/setting_item_selector" >

        <TextView
            style="@style/MySet"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/setting_about" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/right" />
    </RelativeLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
        android:visibility="gone"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="#eee" />
     <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
       
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:background="#eee" />

    <RelativeLayout
        android:id="@+id/rl_friends"
        android:layout_width="fill_parent"
      
        android:layout_height="wrap_content"
        android:background="@drawable/setting_item_selector" >

        <TextView
            style="@style/MySet"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp" 
            android:text="@string/setting_friends" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/right" />
    </RelativeLayout>
    <View
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"
       
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:background="#eee" />

    <RelativeLayout
        android:id="@+id/rl_logout"
        android:layout_width="fill_parent"
        android:visibility="gone"
        android:layout_height="wrap_content"
        android:background="@drawable/setting_item_selector" >

        <TextView
            style="@style/MySet"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="20dp"
            android:paddingRight="20dp" 
            android:text="@string/setting_logout" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:background="@drawable/right" />
    </RelativeLayout>

    </LinearLayout>
</ScrollView>
</LinearLayout>