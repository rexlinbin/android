package com.bccv.zhuiying.model;

import java.util.List;

public class MovieType {
	private List<MovieArea> countries;
	private List<MovieYear> year;
	private List<MovieGenres> type;

	

	public List<MovieArea> getCountries() {
		return countries;
	}

	public void setCountries(List<MovieArea> countries) {
		this.countries = countries;
	}

	public List<MovieYear> getYear() {
		return year;
	}

	public void setYear(List<MovieYear> year) {
		this.year = year;
	}

	public List<MovieGenres> getType() {
		return type;
	}

	public void setType(List<MovieGenres> type) {
		this.type = type;
	}

}
